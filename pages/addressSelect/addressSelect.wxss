/* pages/addressSelect/addressSelect.wxss */
.select{
	width: 100%;
	height: auto;
}
.select-item{
	width: 100%;
	height: 100rpx;
	line-height: 100rpx;
	display: flex;
	border-bottom: 2rpx solid #f3f3f3;
}
.select-item>view{
	flex: 1;
	font-size: 28rpx;
	color: #333;
	margin: 0 30rpx;
}
.select-item>image{
	width: 17rpx;
	height: 30rpx;
	margin: 35rpx 30rpx 35rpx 0;
}
.select-item-xuan{
	width: 100%;
	height: 100rpx;
	line-height: 100rpx;
	display: flex;
	border-bottom: 2rpx solid #f3f3f3;
}
.select-item-zhong{
	width: 130rpx;
	color: #666;
	font-size: 24rpx;
	margin-left: 30rpx;
}
.select-item-xuan>view:last-child{
	flex: 1;
	font-size: 28rpx;
	color: #333;
	margin-right: 30rpx;
}