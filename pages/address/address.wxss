/* pages/address/address.wxss */
page{
	background: #f3f3f3;
}
.address{
	width: 100%;
	height: auto;
}
.fix-iphonex-button {
  content: ' ';
  position: fixed;
  bottom: 0 !important;
  height: 68rpx !important;
  width: 100%;
  background: #fff;
  z-index: 9999999999;
}

.address-list{
	width: 100%;
	height: auto;
	background: #fff;
	margin-bottom: 120rpx;
}
.address-one{
	width: 100%;
	height: auto;
	display: flex;
	border-bottom: 2rpx solid #f3f3f3;
	padding: 55rpx 0 35rpx 0;
}
.address-one-left{
	width: 580rpx;
	height: auto;
	margin: 0 30rpx;
}
.address-one-left-name{
	width: 100%;
	height: auto;
	font-size: 28rpx;
	color: #333;
}
.address-one-left-name>text{
	font-size: 28rpx;
	color: #666;
	padding-left: 60rpx;
}
.address-one-left-nr{
	width: 100%;
	height: auto;
	display: flex;
	flex-wrap: wrap;
	margin-top: 15rpx;
}
.address-left-default{
	width: 70rpx;
	height: 30rpx;
	line-height: 30rpx;
	background: #FDEFEC;
	border-radius: 5rpx;
	font-size: 24rpx;
	color: #E95355;
	text-align: center;
	margin-right: 10rpx;
}
.address-left-default1{
	width: 70rpx;
	height: 30rpx;
	line-height: 30rpx;
	background: #fff;
	border-radius: 5rpx;
	font-size: 24rpx;
	color: #E95355;
	text-align: center;
	margin-right: 10rpx;
}
.add-one-left{
	width: 450rpx;
	height: auto;
	line-height: 30rpx;
	font-size: 28rpx;
	color: #333;
}
.address-one-center{
	width: 2rpx;
	height: 110rpx;
	background: #DDDDDD;
}
.address-one-right{
	width: 160rpx;
	height: 110rpx;
	line-height: 110rpx;
	text-align: center;
	font-size: 28rpx;
	color: #666;
}
.address-new{
	width: 100%;
	height: 90rpx;
	line-height: 90rpx;
	text-align: center;
	background: #E95355;
	font-size: 30rpx;
	color: #fff;
	position: fixed;
	bottom: 0;
	left: 0;
	z-index: 999;
}
.address-wu{
	width: 100%;
	height: auto;
	margin-top: 350rpx;
}
.address-wu>image{
	width: 208rpx;
	height: 208rpx;
	margin: 0 271rpx 35rpx 271rpx;
}
.address-wu>view{
	font-size: 32rpx;
	color: #666;
	text-align: center;
}